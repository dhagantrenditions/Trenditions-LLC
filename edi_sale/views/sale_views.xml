<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_order_form_inherit_edi_sale" model="ir.ui.view">
        <field name="name">sale.order.form.inherit.edi.sale</field>
        <field name="model">sale.order</field>
        <field name="inherit_id" ref="sale.view_order_form"/>
        <field name="arch" type="xml">
            <data>
                <field name="payment_term_id" position="after">
                    <field name="tra_store"/>
                    <field name="is_edi_order" invisible="1"/>
                </field>
            </data>
        </field>
    </record>
    <menuitem id="sale_order_edi"
        parent="sale.sale_order_menu"
        sequence="10"
        action="action_sale_order_edi" />
</odoo>
